{
	"compilerOptions": {
		"target": "es2019",
		"lib": ["ES2019"],
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"strict": true,
		"outDir": "out",
		"rootDir": "src",
		"composite": true,
		"baseUrl": "./src",
		"paths": {
			"@analyzer/*": ["./analyzer/*"],
			"@common/*": ["./analyzer/common/*"],
			"@nodes/*": ["./analyzer/nodes/*"],
		}
	},
	"include": ["src"],
	"exclude": ["node_modules", ".vscode-test"],
	"references": []
}
