{
	"name": "solidity-extension",
	"description": "",
	"license": "MIT",
	"version": "1.0.0",
	"activationEvents": [
		"onLanguage:solidity"
	],
	"main": "./client/out/extension",
	"engines": {
		"vscode": "^1.54.3"
	},
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Solidity configuration"
		},
		"languages": [
			{
				"id": "solidity",
				"aliases": [
					"Solidity",
					"solidity"
				],
				"extensions": [
					".sol"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"postinstall": "cd client && yarn install && cd ../server && yarn install && cd .."
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^2.3.0",
		"eslint": "^6.4.0",
		"typescript": "^4.2.2"
	},
	"dependencies": {}
}
